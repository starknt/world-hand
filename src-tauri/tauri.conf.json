{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run generate",
    "devPath": "http://localhost:1420",
    "distDir": "../.output/public",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "World Terminal",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.personal.starknt",
      "targets": "all",
      "iOS": {
        "developmentTeam": "starknt"
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "World Terminal",
        "width": 800,
        "height": 600
      }
    ]
  }
}
