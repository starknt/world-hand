{
  "name": "@terminal/ui-kit",
  "private": true,
  "exports": {
    ".": {
      "require": "./module.cjs",
      "import": "./dist/module.mjs"
    },
    "./module": {
      "require": "./module.cjs",
      "import": "./dist/module.mjs"
    },
    "./unocss": {
      "types": "./dist/unocss.d.ts",
      "import": "./dist/unocss.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
  ],
  "scripts": {
    "build": "nuxt-build-module",
    "stub": "nuxt-build-module --stub",
    "dev": "nuxi dev playground",
    "playground:build": "nuxi generate playground"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@nuxt/kit": "^3.4.3",
    "@nuxtjs/color-mode": "^3.2.0",
    "@unocss/core": "^0.51.12",
    "@unocss/nuxt": "^0.51.12",
    "@unocss/preset-attributify": "^0.51.12",
    "@unocss/preset-icons": "^0.51.12",
    "@unocss/preset-mini": "^0.51.12",
    "@unocss/reset": "^0.51.12",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "defu": "^6.1.2",
    "unocss": "^0.51.12",
    "unocss-preset-theme": "^0.6.0",
    "v-lazy-show": "^0.2.3"
  },
  "devDependencies": {
    "nuxt": "^3.4.3"
  }
}
